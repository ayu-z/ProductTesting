
config interface 'loopback'
	option device 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option ula_prefix 'fd6b:b47f:66be::/48'
	option multipath 'enable'

config device
	option name 'br-lan'
	option type 'bridge'
	list ports 'eth0'
	list ports 'eth1'
	list ports 'eth2'

config device
	option name 'eth0'
	option macaddr '88:1c:2c:3c:45:13'

config device
	option name 'eth1'
	option macaddr '88:1c:2c:3c:45:13'

config device
	option name 'eth2'
	option macaddr '88:1c:2c:3c:45:13'

config device
	option name 'eth3'
	option macaddr '88:1c:2c:3c:45:13'

config interface 'lan'
	option device 'br-lan'
	option proto 'static'
	option ipaddr '192.168.11.1'
	option netmask '255.255.255.0'
	option ip6assign '60'

config device
	option name 'eth4'
	option macaddr '88:1c:2c:3c:45:14'

config interface 'wan'
	option device 'eth3'
	option proto 'dhcp'
	option metric '1'
	option multipath 'on'

config interface 'wan6'
	option device 'eth4'
	option proto 'dhcpv6'
	option multipath 'on'

config interface 'lte1'
	option proto 'dhcp'
	option metric '4'
	option multipath 'on'
	option device 'NULL'

config interface 'lte2'
	option proto 'dhcp'
	option metric '5'
	option multipath 'on'
	option device 'NULL'

config interface 'lte3'
	option proto 'dhcp'
	option metric '6'
	option multipath 'on'
	option device 'NULL'

config interface 'lte4'
	option proto 'dhcp'
	option metric '7'
	option multipath 'on'
	option device 'wwan0'

config interface 'vpn'
	option proto 'dhcp'
	option device 'tun0'
	option metric '0'
	option multipath 'off'

config interface 'wwanA'
	option proto 'dhcp'
	option device 'wlan00'
	option metric '2'
	option multipath 'on'

config interface 'wwanB'
	option proto 'dhcp'
	option device 'wlan11'
	option metric '3'
	option multipath 'on'

