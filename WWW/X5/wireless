
config wifi-device 'radio0'
	option type 'mac80211'
	option macaddr '88:1c:2c:3c:45:13'
	option channel '36'
	option band '5g'
	option htmode 'HE80'
	option disabled '0'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'X8-5G-3C4513'
	option encryption 'psk2'
	option key '12345678'
	option ifname 'wlan0'

config wifi-device 'radio1'
	option type 'mac80211'
	option macaddr '88:1c:2c:3c:45:14'
	option channel '1'
	option band '2g'
	option noscan '1'
	option ht_coex '1'
	option htmode 'HE40'
	option disabled '0'

config wifi-iface 'default_radio1'
	option device 'radio1'
	option network 'lan'
	option mode 'ap'
	option ssid 'X8-2G-3C4514'
	option encryption 'psk2'
	option key '12345678'
	option ifname 'wlan1'

config wifi-iface 'sta1'
	option device 'radio0'
	option ifname 'wlan00'
	option mode 'sta'
	option network 'wwanA'
	option ssid 'xxxx'
	option encryption 'psk2'
	option key '12345678'
	option disabled '1'

config wifi-iface 'sta2'
	option device 'radio1'
	option ifname 'wlan11'
	option mode 'sta'
	option network 'wwanB'
	option ssid 'xxxx'
	option encryption 'psk2'
	option key '12345678'
	option disabled '1'

